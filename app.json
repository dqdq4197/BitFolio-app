{
  "expo": {
    "name": "bitfolio",
    "slug": "bitfolio",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/bitfolio_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.bitfolio.cryptocurrencies.ios",
      "buildNumber": "1.0.3",
      // "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": [
        "applinks:bitfolio.page.link"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#121212"
      },
      "versionCode": 1,
      "package": "com.bitfolio.cryptocurrencies.ios"
    },
    // "plugins": [
    //   "@react-native-firebase/app"
    // ],
    "web": {
      "favicon": "./assets/splash.png"
    },
    "description": "cryptocurrencies"
  }
