{
  "expo": {
    "name": "bitfolio",
    "slug": "bitfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bitfolio_icon.png",
    "splash": {
      "image": "./assets/bitfolio_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bitfolio.scribbles.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bitfolio_splash.png",
        "backgroundColor": "#121212"
      }
    },
    "web": {
      "favicon": "./assets/bitfolio_splash.png"
    },
    "description": ""
  }
}
